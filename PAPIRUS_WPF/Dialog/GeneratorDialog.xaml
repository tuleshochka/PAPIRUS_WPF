                <DataGridTextColumn x:Name="ColumnParam"  Header="Параметр" Binding="{Binding columnParam}" IsReadOnly="True"/>
                <DataGridTextColumn x:Name="ColumnDopusk"  Header="Допуск, %" Binding="{Binding columnDopusk}" Width="80" />
            </DataGrid.Columns>
        </DataGrid>


