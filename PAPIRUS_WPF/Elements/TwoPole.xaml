<Page x:Class="PAPIRUS_WPF.Elements.TwoPole"
      xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
      xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
      xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
      xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
      xmlns:local="clr-namespace:PAPIRUS_WPF.Elements"
      mc:Ignorable="d" 
      d:DesignHeight="50" d:DesignWidth="100"
      Title="TwoPol">

    <Grid x:Name="TwoPol">
        <Border CornerRadius="7" BorderBrush="#FF8D8D8D" Margin="6" BorderThickness="1" HorizontalAlignment="Stretch" VerticalAlignment="Stretch">
        </Border>
        <local:Output x:Name="LeftInput" HorizontalAlignment="Left" VerticalAlignment="Center"/>
    </Grid>
</Page>
