<Papirus:Object x:Class="PAPIRUS_WPF.Elements.two_pole"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
             xmlns:local="clr-namespace:PAPIRUS_WPF.Elements"
             xmlns:Papirus="clr-namespace:PAPIRUS_WPF" 
             mc:Ignorable="d" 
           Height="52" Width="74">

    <Grid x:Name="TwoPol">
        <Border x:Name="SelectBorder" BorderBrush="{StaticResource MarkerStroke}" BorderThickness="2" Visibility="Collapsed">
        </Border>
        <local:Output x:Name="LeftInput" HorizontalAlignment="Left" VerticalAlignment="Center" Margin="4"/>
        <Border CornerRadius="5" BorderBrush="Black" Margin="11" BorderThickness="1" HorizontalAlignment="Stretch" VerticalAlignment="Stretch">

            <Rectangle x:Name="LEDRect" RadiusX="5" RadiusY="5" Fill="White" Margin="0"/>
        </Border>
            
    </Grid>
</Papirus:Object>
